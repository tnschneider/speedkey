{
    "manifest_version": 2,
    "name": "SpeedKey",
    "version": "1.0",
    "description": "Hotkey based speed dial launcher.",
    "icons": {
        "48": "assets/icon.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": [
                "node_modules/vue/dist/vue.js",
                "src/debounce.js",
                "src/constants.js",
                "src/content-script.js"
            ],
            "css": [
                "src/content-style.css"
            ]
        }
    ],
    "background": {
        "scripts": [
            "node_modules/fuse.js/dist/fuse.js",
            "src/constants.js",
            "src/background.js"
        ]
    },
    "commands": {
        "open-launcher": {
            "suggested_key": {
                "default": "Ctrl+Space"
            },
            "description": "Open the launcher."
        }
    },
    "permissions": [
        "bookmarks",
        "tabs",
        "search",
        "activeTab"
    ]
}